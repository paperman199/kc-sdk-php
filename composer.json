{
    "name": "wkf/kc-sdk-php",
    "description": "Modified PHP wrapper for the KeyCloak",
    "keywords": [
        "sdk",
        "admin",
        "identity server"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": ">=5.3",
        "league/oauth2-client": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.13",
        "symfony/dotenv": "^4.3"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php --testdox tests"
    },
    "autoload": {
        "psr-4": {
            "Keycloak\\": "src/"
        }
    }
}
